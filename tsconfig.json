{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": false,
    "allowJs": false,
    "strict": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "jsxImportSource": "theme-ui",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "assets": [
        "./src/assets/*"
      ],
      "components": [
        "./src/components/*"
      ],
      "firebaseConfig": [
        "src/firebaseConfig/*"
      ],
      "pages": [
        "./src/pages/*"
      ],
      "Theme": [
        "./src/Theme/*"
      ],
    }
  },
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ]
}